<div class="container">
  <div class="row mt-5">
    <h2>ジャンル編集</h2>
  </div>
  <div class="row mt-3">
    <%= form_with model: @genre, url: admin_genre_path do |f| %>
      <label for="genre_name">ジャンル名</label>
      <%= f.text_field :name, id:"genre_name" %>
      <%= f.submit "変更を保存", class: "btn btn-success" %>
    <% end %>
  </div>
</div>
